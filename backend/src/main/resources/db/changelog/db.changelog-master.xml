        <addForeignKeyConstraint baseTableName="WORK"
                                 baseColumnNames="SERVICE_ID"
                                 constraintName="fk_work_service"
                                 referencedTableName="SERVICES"
                                 referencedColumnNames="ID"
                                 baseTableSchemaName="uat"
                                 onDelete="CASCADE"/>
