            <column name="SERVICE_ID" type="VARCHAR(50)"/>
            <column name="workers_names" type="VARCHAR(50)"/>
        </createTable>

        <addForeignKeyConstraint baseTableName="WORK"
                                 baseColumnNames="workers_names"
