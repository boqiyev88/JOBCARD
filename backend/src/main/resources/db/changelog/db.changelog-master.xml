                                 constraintName="fk_joboverview_pdf_file"
                                 referencedTableName="PDF_FILE"
                                 referencedColumnNames="ID"
                                 baseTableSchemaName="uat"
                                 onDelete="CASCADE"/>
    </changeSet>

    <!-- WORK jadvalini yaratish -->
