            <column name="ZONE" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="MRF" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
            <column name="ACCESS" type="VARCHAR(50)">
