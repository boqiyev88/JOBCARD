        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="WORK" schemaName="uat"/>
            </not>
        </preConditions>
        <createTable tableName="WORK" schemaName="uat">
            <column name="ID" type="VARCHAR(50)">
